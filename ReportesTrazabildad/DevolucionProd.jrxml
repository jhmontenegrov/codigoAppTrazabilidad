	<queryString>
		<![CDATA[SELECT db_proyecto_trazabilidad.almacen.`Ubicacion`,
	db_proyecto_trazabilidad.devolucion.`Cant_devolucion`,
	db_proyecto_trazabilidad.devolucion.`Fech_devolucion`,
