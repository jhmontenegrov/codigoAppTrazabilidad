<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>VentaCtrol</servlet-name>
        <servlet-class>Cotrl.VentaCtrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistroVenta</servlet-name>
        <servlet-class>Cotrl.RegistroVenta</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSecionCtrl1</servlet-name>
        <servlet-class>Cotrl.CerrarSecionCtrl1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesionCtrol</servlet-name>
        <servlet-class>Cotrl.CerrarSesionCtrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UsuarioLogin</servlet-name>
        <servlet-class>Cotrl.UsuarioLogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>IngresarVentCtrol</servlet-name>
        <servlet-class>Cotrl.IngresarVentCtrol</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Recibo</servlet-name>
        <servlet-class>Cotrl.Recibo</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>VentaCtrol</servlet-name>
        <url-pattern>/VentaCtrol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistroVenta</servlet-name>
        <url-pattern>/RegistroVenta</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSecionCtrl1</servlet-name>
        <url-pattern>/CerrarSecionCtrl1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesionCtrol</servlet-name>
        <url-pattern>/CerrarSesionCtrol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UsuarioLogin</servlet-name>
        <url-pattern>/UsuarioLogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>IngresarVentCtrol</servlet-name>
        <url-pattern>/IngresarVentCtrol</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Recibo</servlet-name>
        <url-pattern>/Recibo</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
